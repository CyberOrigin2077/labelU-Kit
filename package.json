{
  "name": "label-u",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.7.0",
  "scripts": {
    "start": "lerna run build:type && lerna run --parallel --stream start",
    "dev": "npm run start",
    "build": "lerna run --scope=@label-u/utils --scope=@label-u/annotation --scope=@label-u/components build",
    "build:type": "lerna run build:type",
    "lint": "lerna run --parallel --stream lint",
    "clean": "lerna clean && rimraf ./packages/**/package-lock.json",
    "commit": "cz",
    "release": "multi-semantic-release --ignore-private-packages"
  },
  "author": "wuhui",
  "contributors": [
    "lijingchi",
    "tanjunbao",
    "chenlu",
    "lihuaqi",
    "luoluo"
  ],
  "keywords": [
    "annotation",
    "canvas",
    "react"
  ],
  "homepage": "https://github.com/opendatalab/labelU.git",
  "bugs": {
    "url": "https://github.com/opendatalab/labelU.git",
    "email": "751569801@qq.com"
  },
  "main": "index.js",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.0.3",
    "lerna": "^4.0.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "multi-semantic-release": "^2.13.0",
    "semantic-release": "^19.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "gauge": "^2.7.4",
    "react-ace": "^10.1.0"
  }
}
